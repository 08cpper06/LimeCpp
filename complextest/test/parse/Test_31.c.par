<Block Name="Global">
	<FunctionDefinition Name="sum" ReturnType="int">
		<Arguments>
			<Detail Type="int" Name="lhs"/>
			<Detail Type="int" Name="rhs"/>
		</Arguments>
		<Block Name="Block_sum">
			<Return>
				<Operator Type="+">
					<Variable Type="int" Name="lhs"></Variable>
					<Variable Type="int" Name="rhs"></Variable>
				</Operator>
			</Return>
		</Block>
	</FunctionDefinition>
	<FunctionDefinition Name="main" ReturnType="int">
		<Arguments></Arguments>
		<Block Name="Block_main">
			<VariableDefinition Type="int" Name="arr" ArrayCount="10"></VariableDefinition>
			<For>
				<Initialize>
					<VariableDefinition Type="int" Name="i">
						<Val Type="int">10</Val>
					</VariableDefinition>
				</Initialize>
				<Condition>
					<Relational Type=">=">
						<Variable Type="int" Name="i"></Variable>
						<Val Type="int">0</Val>
					</Relational>
				</Condition>
				<Continus>
					<PosifixUnary Operator=-->
						<Variable Type="int" Name="i"></Variable>
					</PosifixUnary>
				</Continus>
				<Block Name="Anonymous_0">
					<Error Line=1 Message="expected a `]`"/>
				</Block>
			</For>
			<Return>
				<Val Type="int">0</Val>
			</Return>
		</Block>
	</FunctionDefinition>
</Block>
